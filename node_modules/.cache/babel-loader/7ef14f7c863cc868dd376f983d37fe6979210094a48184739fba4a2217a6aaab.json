{"ast":null,"code":"// @ is an alias to /src\nimport Banner from '@/components/Banner.vue';\nimport Productos from '@/components/Productos.vue';\nimport { useStore } from 'vuex';\nimport { onMounted, computed, ref } from 'vue';\nimport Filtros from '@/components/Filtros.vue';\nimport Nosotros from '@/components/Nosotros.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    Productos,\n    Banner,\n    Filtros,\n    Nosotros\n  },\n  setup() {\n    const store = useStore();\n    const productos = ref([]);\n    const isLoading = ref(true);\n    console.log('isLoading:', isLoading.value);\n    console.log('productos:', productos.value);\n    onMounted(async () => {\n      if (!store.state.productos.length) {\n        // Aquí podrías disparar una acción o llamada a la API para cargar los productos si no están cargados\n        await store.dispatch('fetchProductos');\n      }\n      productos.value = store.state.productos; // Acceso correcto al estado de Vuex\n      console.log('Productos después del fetch:', productos.value); // Verifica si los productos fueron cargados\n\n      // Cambia el estado de isLoading a false cuando los productos estén cargados\n      isLoading.value = false;\n      console.log('isLoading (después del fetch):', isLoading.value);\n    });\n    return {\n      productos,\n      isLoading\n    };\n  }\n};","map":{"version":3,"names":["Banner","Productos","useStore","onMounted","computed","ref","Filtros","Nosotros","name","components","setup","store","productos","isLoading","console","log","value","state","length","dispatch"],"sources":["C:\\Users\\maxig\\Documents\\proyectos\\Vue.js\\taste-2024\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n    <Banner/>\n    <Filtros/>\n    <div v-if=\"isLoading\">\n      cargando...\n    </div>\n    <div v-else class=\"container\">\n    <div class=\"row\">\n          <Productos v-for=\"producto in productos\" \n          :producto=\"producto\" :key=\"producto.id\" />\n    </div>\n    </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport Banner from '@/components/Banner.vue'\nimport Productos from '@/components/Productos.vue'\nimport {useStore} from 'vuex'\nimport {onMounted, computed, ref} from 'vue'\nimport Filtros from '@/components/Filtros.vue'\nimport Nosotros from '@/components/Nosotros.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    Productos,\n    Banner,\n    Filtros,\n    Nosotros\n  },\n  setup() {\n    const store = useStore()\n        const productos = ref([]);\n        const isLoading = ref(true);\n        console.log('isLoading:', isLoading.value);\n        console.log('productos:', productos.value);\n        onMounted(async() => {\n        if (!store.state.productos.length) {\n        // Aquí podrías disparar una acción o llamada a la API para cargar los productos si no están cargados\n        await store.dispatch('fetchProductos');\n      }\n        productos.value = store.state.productos; // Acceso correcto al estado de Vuex\n      console.log('Productos después del fetch:', productos.value); // Verifica si los productos fueron cargados\n\n      // Cambia el estado de isLoading a false cuando los productos estén cargados\n      isLoading.value = false;\n      console.log('isLoading (después del fetch):', isLoading.value);\n        });\n        \n        return {productos, isLoading}\n  }\n}\n</script>\n"],"mappings":"AAeA;AACA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,SAAQC,QAAQ,QAAO,MAAK;AAC5B,SAAQC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAI;AAC3C,OAAOC,OAAM,MAAO,0BAAyB;AAC7C,OAAOC,QAAO,MAAO,2BAA0B;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVR,SAAS;IACTD,MAAM;IACNM,OAAO;IACPC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIT,QAAQ,CAAC;IACnB,MAAMU,SAAQ,GAAIP,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMQ,SAAQ,GAAIR,GAAG,CAAC,IAAI,CAAC;IAC3BS,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEF,SAAS,CAACG,KAAK,CAAC;IAC1CF,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEH,SAAS,CAACI,KAAK,CAAC;IAC1Cb,SAAS,CAAC,YAAW;MACrB,IAAI,CAACQ,