f(true);\n        onMounted(async() => {\n        if (!store.state.productos.length) {\n        // Aquí podrías disparar una acción o llamada a la API para cargar los productos si no están cargados\n        await store.dispatch('fetchProductos'); // Asegúrate de tener esta acción en Vuex\n      }\n        console.log(store.state.productos)\n        isLoading.value= false;\n        });\n        const store = useStore()\n        return {productos}\n  }\n}\n</script>\n"],"mappings":"AAaA;AACA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,SAAQC,QAAQ,QAAO,MAAK;AAC5B,SAAQC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAI;AAC3C,OAAOC,OAAM,MAAO,0BAAyB;AAE7C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVP,SAAS;IACTD,MAAM;IACNM;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACF,MAAMC,SAAQ,GAAIN,QAAQ,CAAC,MAAKO,KAAK,CAACC,KAAK,CAACF,SAAS;IACrD,MAAMG,SAAQ,GAAIR,GAAG,CAAC,IAAI,CAAC;IAC3BF,SAAS,CAAC,YAAW;MACrB,IAAI,CAACQ,KAAK,CAACC,KAAK,CAACF,SAAS,CAACI,MAAM,EAAE;QACnC;QACA,MAAMH,KAAK,CAACI,QAAQ,CAAC,gBAAgB,CAAC,EAAE;MAC1C;MACEC,OAAO,CAACC,GAAG,CAACN,KAAK,CAACC,KAAK,CAACF,SAAS;MACjCG,SAAS,CAACK,KAAK,GAAE,KAAK;IACtB,CAAC,CAAC;IACF,MAAMP,KAAI,GAAIT,QAAQ,CAAC;IACvB,OAAO;MAACQ;IAAS;EACvB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}