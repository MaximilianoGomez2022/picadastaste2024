{"ast":null,"code":"// @ is an alias to /src\nimport Banner from '@/components/Banner.vue';\nimport Productos from '@/components/Productos.vue';\nimport { useStore } from 'vuex';\nimport { onMounted, computed } from 'vue';\nimport Filtros from '@/components/Filtros.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    Productos,\n    Banner,\n    Filtros\n  },\n  setUp() {\n    onMounted(async () => {\n      if (!store.state.productos.length) {\n        // Aquí podrías disparar una acción o llamada a la API para cargar los productos si no están cargados\n        await store.dispatch('fetchProductos'); // Asegúrate de tener esta acción en Vuex\n      }\n    });\n    const store = useStore();\n    const productos = computed(() => store.state.productos);\n    return {\n      productos\n    };\n  }\n};","map":{"version":3,"names":["Banner","Productos","useStore","onMounted","computed","Filtros","name","components","setUp","store","state","productos","length","dispatch"],"sources":["C:\\Users\\maxig\\Documents\\proyectos\\Vue.js\\taste-2024\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n    <Banner/>\n    <Filtros/>\n    <div v-if=\"productos && productos.length > 0\" >\n    <Productos v-for=\"producto of productos\" \n    :producto=\"producto\" :key=\"producto.id\" />\n    </div>\n    <div v-else>Cargando</div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport Banner from '@/components/Banner.vue'\nimport Productos from '@/components/Productos.vue'\nimport {useStore} from 'vuex'\nimport {onMounted, computed} from 'vue'\nimport Filtros from '@/components/Filtros.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    Productos,\n    Banner,\n    Filtros\n  },\n  setUp() {\n        onMounted(async() => {\n        if (!store.state.productos.length) {\n        // Aquí podrías disparar una acción o llamada a la API para cargar los productos si no están cargados\n        await store.dispatch('fetchProductos'); // Asegúrate de tener esta acción en Vuex\n      }\n        });\n        const store = useStore()\n        const productos = computed(()=> store.state.productos)\n        return {productos}\n  }\n}\n</script>\n"],"mappings":"AAWA;AACA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,SAAQC,QAAQ,QAAO,MAAK;AAC5B,SAAQC,SAAS,EAAEC,QAAQ,QAAO,KAAI;AACtC,OAAOC,OAAM,MAAO,0BAAyB;AAE7C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVN,SAAS;IACTD,MAAM;IACNK;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACFL,SAAS,CAAC,YAAW;MACrB,IAAI,CAACM,KAAK,CAACC,KAAK,CAACC,SAAS,CAACC,MAAM,EAAE;QACnC;QACA,MAAMH,KAAK,CAACI,QAAQ,CAAC,gBAAgB,CAAC,EAAE;MAC1C;IACE,CAAC,CAAC;IACF,MAAMJ,KAAI,GAAIP,QAAQ,CAAC;IACvB,MAAMS,SAAQ,GAAIP,QAAQ,CAAC,MAAKK,KAAK,CAACC,KAAK,CAACC,SAAS;IACrD,OAAO;MAACA;IAAS;EACvB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}